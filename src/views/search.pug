extends layouts/main
include mixins/videoBlock 

block content
    .search__header
        p Search for #{searchBy}

    .search__videos
        if videos.length === 0
            h5 No video found
        each item in videos
            +videoBlock({
                title: item.title,
                views: item.views,
                videoFile: item.videoFile,
                id: item.id
            })

